{
    "name": "ibuki-extensions-compiler",
    "version": "1.0.0",
    "description": "Compiler and tester for Ibuki scripts",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "build": "npm run test && node index.js -i \"../../src\" -o \"../../extensions.json\"",
        "untested": "node index.js -i \"../../src\" -o \"../../extensions.json\"",
        "test": "mocha --input=\"../../src\"",
        "testJson":"mocha --reporter json --input=\"../../src\""
    },
    "author": "LilyStilson",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^8.44.0",
        "eslint-plugin-jest": "^27.2.3",
        "mocha": "^10.2.0"
    },
    "dependencies": {
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.4.0",
        "chalk": "^5.3.0",
        "esprima": "^4.0.1",
        "node": "^20.3.1"
    }
}
